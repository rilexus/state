  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.26.0",
    "gulp-format-md": "^0.1.9",
    "jest": "^29.7.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1"
  },
  "keywords": [
    "state",
    "subscribe",
    "statefull"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "verb"
    ]
