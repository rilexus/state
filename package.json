{
  "name": "statefull",
  "description": "Subscribe to state changes.",
  "version": "1.0.1",
  "homepage": "https://github.com/rilexus/state",
  "author": "Stanislav Panchenko (https://github.com/rilexus)",
  "contributors": [
    "Stanislav Panchenko (https://github.com/rilexus)"
  ],
  "repository": "rilexus/state",
  "bugs": {
    "url": "https://github.com/rilexus/state/issues"
  },
  "license": "MIT",
  "main": "index.cjs.js",
  "module": "index.js",
  "files": [
    "index.cjs.js",
    "index.js"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "build": "babel index.js --out-file index.cjs.js",
